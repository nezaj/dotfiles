[credential]
    helper = osxkeychain
[push]
    default = upstream
[color]
    ui = true
[color "grep"]
    filename=magenta
[rerere]
    enabled = true
[pack]
    threads = 0
[merge]
    conflictstyle = diff3
[alias]
    co = checkout
    br = branch
    ci = commit
    st = status
    unstage = reset HEAD --
    last = log -1 HEAD
    graph = log --graph --decorate --oneline
